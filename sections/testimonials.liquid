<div class="testimonials">
  <div class="testimonials__title">
    <span class="line"></span>
    <span class="diamond"></span>
    <h3>{{ section.settings.title }}</h3>
    <span class="diamond"></span>
    <span class="line"></span>
  </div>
</div>

{% schema %}
  {
    "name": "testimonials",
    "tag": "section",
    "class": "testimonials",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Section Title",
        "default": "Customer Says"
      }
    ]
  }
{% endschema %}